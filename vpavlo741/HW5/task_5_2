number = int(input("Введіть число для розрахунку послідовності Фібоначчі"))
fibonacci_1 = 0
fibonacci_2 = 1
print(fibonacci_1, end=' ')

while fibonacci_2 <= number:
    print(fibonacci_2, end=' ')
    fibonacci_1, fibonacci_2 = fibonacci_2, fibonacci_1 + fibonacci_2


number = int(input("Введіть порядковий номер числа Фібоначчі для його визначення"))
fibo_1 = 0
fibo_2 = 1
сount = 0
while сount < number-2:
    fibo_1, fibo_2 = fibo_2, fibo_1 + fibo_2
    сount += 1
print(fibo_2)